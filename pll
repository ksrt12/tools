#!/bin/bash
# $1 - repo
# $2 - tag
# $3 - project

case $1 in
	*google*|AOSP|aosp)
		repo="https://android.googlesource.com/platform";
		tag=android-$2;
		;;
	*codeaurora*|CAF|caf)
		repo="https://source.codeaurora.org/quic/la/platform";
		tag=$2;
		;;
esac;
if [ -z "$3" ]; then
	proj=$(echo `pwd` | sed -e 's/\/home\/ksrt12\/havoc\///');
else
	proj=$3;
fi;
$echo git pull --signoff --gpg-sign --stat --log --commit $repo/$proj $tag;
